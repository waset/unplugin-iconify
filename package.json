{
  "name": "@waset/unplugin-iconify",
  "type": "module",
  "version": "1.3.0",
  "packageManager": "pnpm@9.12.3",
  "description": "",
  "author": "waset <waset@foxmail.com>",
  "license": "MIT",
  "homepage": "https://github.com/waset/unplugin-iconify#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/waset/unplugin-iconify.git"
  },
  "bugs": {
    "url": "https://github.com/waset/unplugin-iconify/issues"
  },
  "keywords": [
    "astro",
    "esbuild",
    "farm",
    "icon",
    "iconify",
    "icons",
    "loader",
    "nuxt",
    "rollup",
    "rspack",
    "transform",
    "unocss",
    "unplugin",
    "vite",
    "webpack"
  ],
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    },
    "./*": "./*",
    "./astro": {
      "import": "./dist/astro.js",
      "require": "./dist/astro.cjs"
    },
    "./env": {
      "import": "./dist/env.js",
      "require": "./dist/env.cjs"
    },
    "./esbuild": {
      "import": "./dist/esbuild.js",
      "require": "./dist/esbuild.cjs"
    },
    "./farm": {
      "import": "./dist/farm.js",
      "require": "./dist/farm.cjs"
    },
    "./loader": {
      "import": "./dist/loader.js",
      "require": "./dist/loader.cjs"
    },
    "./nuxt": {
      "import": "./dist/nuxt.js",
      "require": "./dist/nuxt.cjs"
    },
    "./rollup": {
      "import": "./dist/rollup.js",
      "require": "./dist/rollup.cjs"
    },
    "./rspack": {
      "import": "./dist/rspack.js",
      "require": "./dist/rspack.cjs"
    },
    "./types": {
      "import": "./dist/types.js",
      "require": "./dist/types.cjs"
    },
    "./vite": {
      "import": "./dist/vite.js",
      "require": "./dist/vite.cjs"
    },
    "./webpack": {
      "import": "./dist/webpack.js",
      "require": "./dist/webpack.cjs"
    }
  },
  "main": "dist/index.cjs",
  "module": "dist/index.js",
  "types": "dist/index.d.ts",
  "typesVersions": {
    "*": {
      "*": [
        "./*",
        "./dist/*"
      ]
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsup",
    "build:fix": "esno scripts/postbuild.ts",
    "dev": "tsup --watch src",
    "lint": "eslint .",
    "play": "npm -C playground run dev",
    "prepublishOnly": "npm run build",
    "release": "bumpp && npm publish",
    "start": "esno src/index.ts",
    "test": "vitest"
  },
  "peerDependencies": {
    "@farmfe/core": ">=1",
    "@nuxt/kit": "^3",
    "@nuxt/schema": "^3",
    "esbuild": "*",
    "rollup": "^3 || ^4.0.0",
    "vite": ">=3",
    "webpack": "^4 || ^5"
  },
  "peerDependenciesMeta": {
    "@farmfe/core": {
      "optional": true
    },
    "@nuxt/kit": {
      "optional": true
    },
    "@nuxt/schema": {
      "optional": true
    },
    "esbuild": {
      "optional": true
    },
    "rollup": {
      "optional": true
    },
    "vite": {
      "optional": true
    },
    "webpack": {
      "optional": true
    }
  },
  "dependencies": {
    "@iconify/tools": "^4.0.7",
    "jsonc-parser": "^3.3.1",
    "unplugin": "^2.0.0"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^3.8.0",
    "@nuxt/kit": "^3.13.2",
    "@nuxt/schema": "^3.13.2",
    "@types/node": "^22.7.6",
    "bumpp": "^9.7.1",
    "chalk": "^5.3.0",
    "eslint": "^9.12.0",
    "esno": "^4.8.0",
    "fast-glob": "^3.3.2",
    "nodemon": "^3.1.7",
    "rollup": "^4.24.0",
    "tsup": "^8.3.0",
    "typescript": "^5.6.3",
    "vite": "^5.4.9",
    "vitest": "^2.1.3",
    "webpack": "^5.95.0"
  },
  "public": true,
  "publishConfig": {
    "access": "public"
  }
}
